'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-thumbnail'
    'body': """
    <figure class="uk-thumbnail">
      <img src="$1" alt="$2">
      <figcaption class="uk-thumbnail-caption">$3</figcaption>
    </figure>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-thumbnail'
    'body': """
    figure.uk-thumbnail
      img(src="$1" alt="$2")
      figcaption.uk-thumbnail-caption $3
    """
