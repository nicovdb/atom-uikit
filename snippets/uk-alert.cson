'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-alert'
    'body': """
    <div class="uk-alert" data-uk-alert>
      <a href="#" class="uk-alert-close uk-close"></a>
      <p>$1</p>
    </div>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-alert'
    'body': """
    div.uk-alert(data-uk-alert="")
      a.uk-alert-close.uk-close(href="#")
      p $1
    """
