'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-sortable'
    'body': """
    <ul class="uk-sortable" data-uk-sortable>
      <li>$1</li>
      <li>$2</li>
    </ul>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-sortable'
    'body': """
    ul.uk-sortable(data-uk-sortable)
      li $1
      li $2
    """
