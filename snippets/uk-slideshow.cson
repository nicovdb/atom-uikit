'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-slideshow'
    'body': """
    <ul class="uk-slideshow" data-uk-slideshow>
      <li><img src="$1" width="$2" height="$3" alt="$4"></li>
    </ul>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-slideshow'
    'body': """
    ul.uk-slideshow(data-uk-slideshow="")
      li
        img(src="$1" width="$2" height="$3" alt="$4")
    """
