'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-offcanvas'
    'body': """
    <a href="#${1:offcanvas}" data-uk-offcanvas>$2</a>
    <div id="${1:offcanvas}" class="uk-offcanvas">
      <div class="uk-offcanvas-bar">$3</div>
    </div>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-offcanvas'
    'body': """
    a(href="#${1:offcanvas}" data-uk-offcanvas="") $2
    div#${1:offcanvas}.uk-offcanvas
      div.uk-offcanvas-bar $3
    """
