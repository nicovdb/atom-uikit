'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-overlay'
    'body': """
    <a class="uk-overlay" href="$1">
      <img src="$2" alt="$3">
      <div class="uk-overlay-panel">$4</div>
    </a>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-overlay'
    'body': """
    a.uk-overlay(href="$1")
      img(src="$2" alt="$3")
      div.uk-overlay-panel $4
    """
