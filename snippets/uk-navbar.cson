'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-navbar'
    'body': """
    <nav class="uk-navbar">
      <ul class="uk-navbar-nav">
        <li class="uk-active"><a href="$1">$2</a></li>
        <li><a href="$3">$4</a></li>
      </ul>
    </nav>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-navbar'
    'body': """
    nav.uk-navbar
      ul.uk-navbar-nav
        li.uk-active
          a(href="$1") $2
        li
          a(href="$3") $4
    """
