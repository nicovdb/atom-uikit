'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-slider'
    'body': """
    <div data-uk-slider>
      <div class="uk-slider-container">
        <ul class="uk-slider">
          <li>$1</li>
          <li>$2</li>
        </ul>
      </div>
    </div>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-slider'
    'body': """
    div(data-uk-slider="")
      div.uk-slider-container
        ul.uk-slider
          li $1
          li $2
    """
