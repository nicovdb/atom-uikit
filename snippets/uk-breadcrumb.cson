'.text.html, .text.html.php, .source.php':
  'UIkit':
    'prefix': 'uk-breadcrumb'
    'body': """
    <ul class="uk-breadcrumb">
      <li><a href="$1">$2</a></li>
      <li><span>$3</span></li>
      <li class="uk-active"><span>$4</span></li>
    </ul>
    """
'.source.jade,.source.pug':
  'UIkit':
    'prefix': 'uk-breadcrumb'
    'body': """
    ul.uk-breadcrumb
      li
        a(href="$1") $2
      li
        span $3
      li.uk-active
        span $4
    """
